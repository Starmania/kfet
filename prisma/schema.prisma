generator client {
    provider        = "prisma-client-js"
}

datasource db {
    provider             = "postgresql"
    url                  = env("DATABASE_URL")
}

model Article {
    id              Int      @id @default(autoincrement())
    name            String
    description     String?
    sell_price      Decimal
    category        String
    image           String?
    favorite        Boolean  @default(false)
    deleted         Boolean  @default(false)
}



model Command {
    id              Int      @id @default(autoincrement())
    title           String
    description     String?
    status          String
    estimated_end   DateTime?
    created_at      DateTime @default(now())
}